{
  "name": "Test-Laptop",
  "override_attributes": {
    "bcpc": {
      "virt_type" : "qemu",
      "ceph": {
        "hdd_disks" : [ "sdb", "sdc" ],
        "ssd_disks" : [ "sdd", "sde" ]
      },
      "domain_name" : "bcpc.example.com",
      "bootstrap": {
        "interface" : "eth0",
        "pxe_interface" : "eth1",
        "server" : "10.0.100.3",
        "dhcp_subnet" : "10.0.100.0",
        "dhcp_range" : false
      },
      "hadoop" : {
        "hdfs" : {
          "dfs_replication_factor" : 1
        },
        "disks" : ["sdb" , "sdc" ],
        "distribution" : {
          "version" : "HDP",
          "key" : "hortonworks.key"
        }
      },
      "repos" : {
        "hortonworks" : "http://public-repo-1.hortonworks.com/HDP/ubuntu12/2.0.11.0",
        "hdp_utils" : "http://public-repo-1.hortonworks.com/HDP-UTILS-1.1.0.16/repos/ubuntu12"
      }
    },
    "kafka": {
      "version": "0.8.1.1",
      "scala_version": "2.9.2",
      "checksum": "",
      "md5_checksum": "",
      "log_dir": "/disk/0"
    },
    "ntp": {
       "servers" : [ "0.pool.ntp.org", "1.pool.ntp.org", "2.pool.ntp.org", "3.pool.ntp.org" ]
    },
    "chef_client": {
      "cache_path": "/var/chef/cache",
      "backup_path": "/var/chef/backup",
      "validation_client_name": "chef-validator",
      "run_path": "/var/chef"
    }
  },
  "json_class": "Chef::Environment",
  "description": "Laptop test environment",
  "cookbook_versions": {
  },
  "chef_type": "environment"
}
